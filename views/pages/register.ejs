<% include ../partials/header %>
	
	<img src="../resources/img/beproudcapaabanner.jpg" class="img-fluid" alt="Registration Page Banner for CU Football">
    <br>
	<br>
	<div class="container">
		<form action="/reg_user" method="post">
			<div class="form-group row">
				<label class="col-sm-2 col-form-label" for="username">User Name</label>
				<div class="col-sm-10">
					<input type="text" class="form-control" name="username" id="username"  placeholder="Enter Name">
				</div>
			</div>
			<div class="form-group row">
				<label  class="col-sm-2 col-form-label" for="pass">Password</label>
				<div class="col-sm-10">
					<input type="password" class="form-control" id="pass" name="pass" placeholder="Password">
				</div>
			</div>
			<input type="submit" value="submit" class="btn btn-primary btn-block">
		</form>
		<br>
	</div>
<% include ../partials/footer %>